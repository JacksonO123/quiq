struct Inner {
  value: string;
  lifetime: int;
}

struct Obj {
  value: int;
  other: Inner[];
}

struct Temp {
  name: string;
  number: int;
  skills: long[];
  more: Obj;
}

Temp str = <Temp>{
  name: "hi",
  number: 123,
  skills: <long>[long(123), long(124)],
  more: <Obj>{
    value: 10,
    other: <Inner>[
      <Inner>{
        value: "epic",
        lifetime: 10
      },
      <Inner>{
        value: "cool",
        lifetime: 5
      }
    ]
  }
};

str.more.value = 2;

ref<Inner[]> arrRef = str.more.other;
arrRef.push(<Inner>{
  value: "more!",
  lifetime: 1000
});

print(arrRef.length);
print(str);
