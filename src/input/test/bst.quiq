struct BstNodeAddParams {
  self: ref<BstNode>;
  val: int;
}

struct BstNode {
  left: union<ref<BstNode>, null>;
  right: union<ref<BstNode>, null>;
  add: fn<BstNodeAddParams, void>;
  value: int;
}

struct BstAddParams {
  self: ref<Bst>;
  val: int;
}

struct Bst {
  root: union<BstNode, null>;
  add: fn<BstAddParams, void>;
  val: int;
}

func createBstNode(int val) BstNode {
  return <BstNode>{
    left: null,
    right: null,
    value: val,
    add: func (ref<BstNode> self, int val) void {
      if (val < clone(self.value)) {
        if (self.left == null) {
          self.left = ref(createBstNode(val));
        } else {
          self.left.add(val);
        }
      } else {
        if (self.right == null) {
          self.right = ref(createBstNode(val));
        } else {
          self.right.add(val);
        }
      }
    }
  }
}

func createBst() Bst {
  return <Bst>{
    root: null,
    val: 1,
    add: func (ref<Bst> self, int val) void {
      if (self.root == null) {
        self.root = createBstNode(val);
      } else {
        self.root.add(val);
      }
    }
  }
}

Bst a = createBst();

for (i; 0; 700) {
  a.add(i);
}

