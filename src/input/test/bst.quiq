struct BstNode {
  left: union<ref<BstNode>, null>;
  right: union<ref<BstNode>, null>;
  value: int;
}

struct BstAddParams {
  self: ref<Bst>;
}

struct Bst {
  root: union<BstNode, null>;
  add: fn<BstAddParams, void>;
  val: int;
}

func createBst() Bst {
  return <Bst>{
    root: null,
    val: 1,
    add: func (ref<Bst> self) void {
      self.val = 2;
    }
  }
}

Bst a = createBst();
a.add();
print(a);
